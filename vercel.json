{
    "headers": [
        {
            "source": "/assets/(.*).js",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "application/javascript"
                }
            ]
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "version": 2,
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}